{
  "name": "jsonfeed-to-atom",
  "version": "1.0.0",
  "description": "Convert a JSON feed to an atom feed",
  "main": "index.js",
  "scripts": {
    "test": "run-s test:*",
    "test:tape": "nyc tape 'test.js' | tap-format-spec",
    "test:lint": "standard",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "debug": "node --nolazy --inspect-brk=9229 node_modules/.bin/tape 'test.js' | tap-format-spec",
    "update": "node generate-snapshot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcomnes/jsonfeed-to-atom.git"
  },
  "keywords": [
    "jsonfeed",
    "atom",
    "rss",
    "xml",
    "json"
  ],
  "author": "Bret Comnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcomnes/jsonfeed-to-atom/issues"
  },
  "homepage": "https://github.com/bcomnes/jsonfeed-to-atom#readme",
  "dependencies": {
    "run-parallel": "^1.1.8",
    "striptags": "^3.1.1",
    "trim-left": "^1.0.1",
    "trim-right": "^1.0.1",
    "xmlbuilder": "^9.0.7"
  },
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "coveralls": "^3.0.0",
    "existy": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.6.0",
    "standard": "^10.0.0",
    "tape": "^4.7.0",
    "tape-run": "^3.0.0",
    "temporary-directory": "^1.0.2"
  }
}
